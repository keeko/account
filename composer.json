{
	"name" : "keeko/account",
	"description" : "Keeko Account Module",
	"type" : "keeko-module",
	"license" : "MIT",
	"authors" : [{
			"name" : "gossi"
		}
	],
	"autoload" : {
		"psr-4" : {
			"keeko\\account\\" : "src/"
		}
	},
	"require" : {
		"php" : ">=5.5",
		"keeko/composer-installer" : "*"
	},
	"require-dev" : {
		"keeko/framework" : "dev-master",
		"keeko/core" : "@dev",
		"composer/composer" : "@dev",
		"propel/propel" : "@dev",
		"puli/repository" : "@beta",
		"puli/composer-plugin" : "@beta",
		"puli/twig-extension" : "@beta",
		"puli/url-generator" : "@beta",
		"puli/discovery" : "@beta"
	},
	"extra" : {
		"keeko" : {
			"module" : {
				"title" : "Keeko Account",
				"class" : "keeko\\account\\AccountModule",
				"extension-points" : {
					"keeko.account.settings" : "schema/settings.json"
				},
				"extensions" : {
					"keeko.account.settings" : [{
							"label" : "profile",
							"icon" : "user",
							"slug" : "slug.settings.profile",
							"domain" : "keeko.account",
							"module" : "keeko/account",
							"action" : "profile"
						}, {
							"label" : "change_password",
							"icon" : "lock",
							"slug" : "slug.settings.change-password",
							"domain" : "keeko.account",
							"module" : "keeko/account",
							"action" : "change-password"
						}, {
							"label" : "sessions",
							"icon" : "lock",
							"slug" : "slug.settings.sessions",
							"domain" : "keeko.account",
							"module" : "keeko/account",
							"action" : "sessions"
					}]
				},
				"actions" : {
					"dashboard" : {
						"title" : "Account Dashboard",
						"class" : "keeko\\account\\action\\DashboardAction",
						"acl" : [
							"user"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\DashboardHtmlResponder"
						},
						"styles" : [
							"/keeko/account/public/css/account.css"
						]
					},
					"index" : {
						"title" : "Account index",
						"class" : "keeko\\account\\action\\IndexAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\IndexHtmlResponder"
						}
					},
					"account-widget" : {
						"title" : "Account Widget",
						"class" : "keeko\\account\\action\\AccountWidgetAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\AccountWidgetHtmlResponder"
						},
						"styles" : [
							"/keeko/account/public/css/account-widget.css"
						]
					},
					"account" : {
						"title" : "Account",
						"class" : "keeko\\account\\action\\AccountAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\AccountHtmlResponder"
						}
					},
					"login" : {
						"title" : "Account Login",
						"class" : "keeko\\account\\action\\LoginAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\LoginHtmlResponder"
						}
					},
					"logout" : {
						"title" : "Account Logout",
						"class" : "keeko\\account\\action\\LogoutAction",
						"acl" : [
							"guest"
						]
					},
					"profile" : {
						"title" : "Account Profile",
						"class" : "keeko\\account\\action\\ProfileAction",
						"acl" : [
							"user"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\ProfileHtmlResponder"
						}
					},
					"settings" : {
						"title" : "Account Settings",
						"class" : "keeko\\account\\action\\SettingsAction",
						"acl" : [
							"user"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\SettingsHtmlResponder"
						},
						"styles" : [
							"/keeko/account/public/css/account.css"
						]
					},
					"change-password" : {
						"title" : "Change Password",
						"class" : "keeko\\account\\action\\ChangePasswordAction",
						"acl" : [
							"user"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\ChangePasswordHtmlResponder"
						}
					},
					"sessions" : {
						"title" : "User Sessions",
						"class" : "keeko\\account\\action\\SessionsAction",
						"acl" : [
							"user"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\SessionsHtmlResponder"
						}
					},
					"forgot-password" : {
						"title" : "Forgot Password",
						"class" : "keeko\\account\\action\\ForgotPasswordAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\ForgotPasswordHtmlResponder"
						}
					},
					"register" : {
						"title" : "Registration",
						"class" : "keeko\\account\\action\\RegisterAction",
						"acl" : [
							"guest"
						],
						"response" : {
							"html" : "keeko\\account\\responder\\RegisterHtmlResponder"
						}
					}
				}
			}
		}
	}
}